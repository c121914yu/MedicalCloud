image{
	height: auto;
}
/* 蒙层样式 */
.mask{
	width:100%;
	height:100%;
	position:fixed;
	background-color:#999;
	z-index:99;
	top:0;
	left:0;
	opacity:0.8;
}
/* 弹框样式 */
.Popup{
	background: #FFFFFF;
	width: 90%;
	border-radius: 10px;
	position: fixed;
	top: 10%;
	left: 5%;
	z-index: 999;
}
