<view class="MyPlans"><block wx:for="{{plans}}" wx:for-item="plan" wx:for-index="index0" wx:key="index0"><view class="plan-item"><view class="head"><text class="name">{{plan.EquipmentName}}</text><text data-event-opts="{{[['tap',[['Setplan',['$0'],[[['plans','',index0,'PlanID']]]]]]]}}" class="change" bindtap="__e">编辑</text></view><view class="content"><view class="begin">{{''+plan.date.year+"/"+plan.date.month+"/"+plan.date.day+'开始,每'}}<text class="frequency">{{plan.Frequency}}</text>天执行</view><block wx:for="{{plan.MedicalInfo}}" wx:for-item="item" wx:for-index="index1" wx:key="index1"><view class="medical-info"><view class="time">{{''+item.time+''}}</view><view class="medicines"><block wx:for="{{item.Medicines}}" wx:for-item="medical" wx:for-index="index2" wx:key="index2"><view class="medical"><block wx:if="{{medical.remark!=''}}"><view class="dot"></view></block><view class="{{[medical.remark===''?'none-remark':'']}}">{{''+medical.name+"*"+medical.amount+''}}</view><block wx:if="{{medical.remark}}"><view class="remark">{{'备注:'+medical.remark+''}}</view></block></view></block></view></view></block></view></view></block></view>