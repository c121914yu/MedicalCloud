
page{
	color: #f4f4f4;
	font-size: 15px;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.list{
	background: #088573;
	-webkit-box-shadow: 1.5px 1.5px 2px #9a9a9a;
	        box-shadow: 1.5px 1.5px 2px #9a9a9a;
	line-height: 1.5;
	width: 95%;
	min-height: 50px;
	margin: 5px auto;
	border-radius: 10px;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.time{
	width: 100%;
	margin-left: 5px;
}
.name{
	max-width: 70%;
	margin-left: 5px;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.list image{
	width: 30px;
	position: absolute;
	margin-left: 80%;
}

