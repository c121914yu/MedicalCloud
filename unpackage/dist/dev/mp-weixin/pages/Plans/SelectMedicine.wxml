<block wx:if="{{show}}"><view class="SelectMedicine data-v-8f6ba230"><view class="mask data-v-8f6ba230"></view><view class="Popup data-v-8f6ba230"><view class="Title data-v-8f6ba230">{{EquipmentInfo.name+"药柜配置"}}</view><view class="Remark data-v-8f6ba230">点击可选择需要使用的药柜,长按可配置药柜信息</view><uni-grid vue-id="1" column="3" showBorder="{{false}}" highlight="{{false}}" class="data-v-8f6ba230" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{Medicines}}" wx:for-item="medical" wx:for-index="index" wx:key="index"><uni-grid-item class="{{['data-v-8f6ba230',medical.active?'Active':'']}}" bind:tap="__e" bind:longpress="__e" bind:touchstart="__e" vue-id="{{('2-'+index)+','+('1')}}" data-event-opts="{{[['^tap',[['PitchMedical',['$0',index],[[['Medicines','',index]]]]]],['^longpress',[['SetMedicineInfo',[index]]]],['^touchstart',[['e0']]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="medical data-v-8f6ba230"><view class="data-v-8f6ba230">{{"药柜编号:"+(index+1)}}</view><view class="data-v-8f6ba230">{{medical.name==''?'暂无存药':medical.name}}</view><view class="data-v-8f6ba230">{{medical.amount==''?'':"数量:"+medical.amount}}</view></view></uni-grid-item></block></uni-grid><set-medicine vue-id="3" show="{{SetMedicine}}" InitialInfo="{{InitialInfo}}" StaticInfo="{{InitialInfo.MedicalInfo[InitialInfo.MedicalIndex]}}" data-event-opts="{{[['^CloseSet',[['CloseSet']]],['^UpdateMedicals',[['UpdateMedicals']]]]}}" bind:CloseSet="__e" bind:UpdateMedicals="__e" class="data-v-8f6ba230" bind:__l="__l"></set-medicine><button type="primary" data-event-opts="{{[['tap',[['finish',['$event']]]]]}}" bindtap="__e" class="data-v-8f6ba230">完成</button></view></view></block>