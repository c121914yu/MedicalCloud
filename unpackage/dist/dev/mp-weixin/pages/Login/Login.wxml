<view><view class="logo"><image src="../../static/logo.png" mode="widthFix"></image></view><view class="info"><image src="../../static/login/phone.png" mode="widthFix"></image><input type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><view style="width:25px;" class="_div"></view></view><view class="info"><image src="../../static/login/password.png" mode="widthFix"></image><input type="text" password="{{!showpassword}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><image class="right" src="{{showpassword==true?'../../static/login/show.png':'../../static/login/hide.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image></view><button class="login-btn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['Login',['$event']]]]]}}" bindgetuserinfo="__e">登录</button><view class="problem"><text data-event-opts="{{[['tap',[['ForgetPassword',['$event']]]]]}}" bindtap="__e">忘记密码</text><text>|</text><text data-event-opts="{{[['tap',[['Register',['$event']]]]]}}" bindtap="__e">注册用户</text></view></view>