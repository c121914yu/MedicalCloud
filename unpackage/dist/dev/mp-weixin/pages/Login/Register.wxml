<view><view class="logo"><image src="../../static/logo.png" mode="widthFix"></image></view><view class="info"><image src="../../static/login/phone.png" mode="widthFix"></image><input type="number" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/><view style="width:25px;" class="_div"></view></view><view class="info"><image src="../../static/login/password.png" mode="widthFix"></image><input type="text" password="{{!showpassword}}" placeholder="请输入密码" maxlength="12" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/><image class="right" src="{{showpassword==true?'../../static/login/show.png':'../../static/login/hide.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></image></view><view class="info"><image src="../../static/login/password.png" mode="widthFix"></image><input type="text" password="{{!showpassword}}" placeholder="请再次输入密码" maxlength="12" data-event-opts="{{[['input',[['__set_model',['','sure_password','$event',[]]]]]]}}" value="{{sure_password}}" bindinput="__e"/><view style="width:25px;" class="_div"></view></view><view class="info"><image src="../../static/login/checkrand.png" mode="widthFix"></image><input type="number" maxlength="6" placeholder="输入验证码" data-event-opts="{{[['input',[['__set_model',['','rand','$event',[]]]]]]}}" value="{{rand}}" bindinput="__e"/><view style="width:25px;" class="_div"></view><view data-event-opts="{{[['tap',[['getrand',['$event']]]]]}}" class="{{['getrand',(second>0)?'wait':'']}}" bindtap="__e">{{SendRand}}</view></view><button class="register-btn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['Register',['$event']]]]]}}" bindgetuserinfo="__e">注册</button></view>